{
  "name": "API Inspector",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Inspect API requests made by websites",

  "permissions": ["webRequest", "storage", "tabs"],
  "host_permissions": ["<all_urls>"],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
